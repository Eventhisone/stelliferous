<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>


<AnchorPane fx:id="rectAnchorPane" 
            fx:controller="controller.CompositionController"
            maxHeight="-Infinity" 
            maxWidth="-Infinity" 
            prefHeight="1280.0" 
            prefWidth="2000.0" 
            xmlns:fx="http://javafx.com/fxml/1" 
            xmlns="http://javafx.com/javafx/8.0.111" >
    <children>
        <AnchorPane fx:id="compositionPane" 
                    onMouseDragged="#paneMouseDrag" 
                    onMousePressed="#paneMouseClick" 
                    onMouseReleased="#paneMouseRelease" 
                    maxHeight="-Infinity" 
                    maxWidth="-Infinity" 
                    prefHeight="1280.0" 
                    prefWidth="2000.0" />
    </children>
</AnchorPane>
